    "!pip install PyPDF2\n",
    "import PyPDF2\n",
    "file = input(\"Enter file Path\")\n",
    "pdfFile = open(file,'rb')\n",
    "pdfReader = PyPDF2.PdfFileReader(pdfFile)\n",
    "print(pdfReader.numPages)\n",
    "pdfObj = pdfReader.getPage(0)\n",
    "print(pdfObj.extractText())\n",
    "pdfFile.close()"
